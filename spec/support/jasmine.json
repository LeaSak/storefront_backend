{
  "spec_dir": "dist",
  "spec_files": [
    "models/tests/*[sS]pec.js",
    "server/routes/api/tests/*[sS]pec.js"
  ],
  "helpers": [
    "helpers/**/*.js"
  ],
  "reporters": [
    {
      "name": "jasmine-spec-reporter#SpecReporter",
      "options": {
        "displayStacktrace": "all"
      }
    }
  ],
  "stopSpecOnExpectationFailure": false,
  "random": true
}
